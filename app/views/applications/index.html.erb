<p id="notice"><%= notice %></p>

<h1>Listing Application</h1>

<table>
  <thead>
    <tr>
      <th>Daterequest</th>
      <th>Dateeventi</th>
      <th>Dateeventf</th>
      <th>Timeevent</th>
      <th>Eventcode</th>
      <th>Namerequesting</th>
      <th>Phonenum</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% applications.each do |application| %>
      <tr>
        <td><%= application.dateRequest %></td>
        <td><%= application.dateEventI %></td>
        <td><%= application.dateEventF %></td>
        <td><%= application.timeEvent %></td>
        <td><%= application.eventCode %></td>
        <td><%= application.nameRequesting %></td>
        <td><%= application.phoneNum %></td>
        <td><%= application.place %></td>
        <td><%= link_to 'Show', application %></td>
        <td><%= link_to 'Edit', edit_application_path(application) %></td>
        <td><%= link_to 'Destroy', application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application', new_application_path %>
